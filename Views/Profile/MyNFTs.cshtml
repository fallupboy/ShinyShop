@model Dictionary<string, string>

@{
    ViewData["Title"] = "MyNFTs";
}

<h1>My NFTs</h1>
@if (Model.Count <= 0)
{
    <h3>You dont have any NFTs</h3>
}
else
{
    <div class="row">
        @foreach (var nft in Model)
        {
            <div class="column" style="float: left; width: 25%; padding: 2px;">
                <img src="@nft.Value" />
                <h4 style="text-align: center;">@nft.Key</h4>
            </div>
        }
    </div>
}


